<Window x:Class="HvZClient.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Hoomanz vs Zombeez" Height="350" Width="525" Background="Black"
        Icon="icon.ico" WindowStyle="ToolWindow"
        SizeChanged="Window_Resized" Closing="Window_Closing" RenderOptions.BitmapScalingMode="HighQuality">
    <Window.Resources>
        <ImageBrush x:Key="human" ImageSource="Images/human.png" />
        <ImageBrush x:Key="zombie" ImageSource="Images/zombie.png" />
        <ImageBrush x:Key="obstacle" ImageSource="Images/obstacle.png" />
        <ImageBrush x:Key="supply" ImageSource="Images/supplyP.png" />
    </Window.Resources>
    <Grid Name="top">
        <Canvas Name="GUIMap" Background="#FFB8F9FF" >
            <Canvas.OpacityMask>
                <ImageBrush ImageSource="Images/map.png"/>
            </Canvas.OpacityMask>
        </Canvas>
    </Grid>
</Window>
