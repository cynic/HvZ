<Window x:Class="HvZ.Client.GameWindow" x:ClassModifier="internal"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Hoomanz vs Zombeez" Height="700" Width="1000" Background="Black"
        Icon="icon.ico"
        SizeChanged="Window_Resized" Closing="Window_Closing" Loaded="Window_Loaded">
    <Grid Name="ground">
        <Canvas>
            <Canvas.Background>
                <ImageBrush ImageSource="images/ground.png" TileMode="Tile" Stretch="None" Viewport="0,0,128,128" ViewportUnits="Absolute" />
            </Canvas.Background>
        </Canvas>
        <Canvas Name="GUIMap" />
    </Grid>
</Window>
