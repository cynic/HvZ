<Window x:Class="HvZClient.GameWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Hoomanz vs Zombeez" Height="350" Width="525" Background="Black"
        Icon="icon.ico"
        SizeChanged="Window_Resized" Closing="Window_Closing" Loaded="Window_Loaded" RenderOptions.BitmapScalingMode="HighQuality">
    <Grid Name="ground">
        <Canvas Name="GUIMap" Background="Transparent"/>
        <Canvas Background="Black" Opacity="0.4">
            <Canvas.OpacityMask>
                <ImageBrush ImageSource="Images/frame.png"/>
            </Canvas.OpacityMask>
        </Canvas>
    </Grid>
</Window>
