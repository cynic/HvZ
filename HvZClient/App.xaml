<Application x:Class="HvZ.Client.App" x:ClassModifier="internal"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             StartupUri="GameWindow.xaml">
    <Application.Resources>
        <ImageBrush x:Key="human" ImageSource="Images/human.png" />
        <ImageBrush x:Key="zombie" ImageSource="Images/zombie.png" />
        <ImageBrush x:Key="spawnpoint" ImageSource="Images/box.png" />
        <ImageBrush x:Key="obstacle" ImageSource="Images/obstacle.png" />
        <ImageBrush x:Key="supply" ImageSource="Images/supplyP.png" />
        <RadialGradientBrush x:Key="missile" GradientOrigin=".5,.8" RadiusX="0.6">
            <GradientStop Color="#002255" Offset="1" />
            <GradientStop Color="#00eeff" Offset="0" />
        </RadialGradientBrush>
    </Application.Resources>
</Application>
